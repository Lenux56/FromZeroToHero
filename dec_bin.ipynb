    "def dec_to_bin(n, res):\n",
    "    if n < 2:\n",
    "        res.append(int(n))\n",
    "        res.reverse()\n",
    "        return res\n",
    "    \n",
    "    res.append(int(n%2))\n",
    "    return dec_to_bin(n//2, res)\n",
    "    "
   ]
